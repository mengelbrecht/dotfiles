"*":
  core:
    disabledPackages: [
      "archive-view"
      "autoflow"
      "background-tips"
      "exception-reporting"
      "feedback"
      "image-view"
      "language-clojure"
      "language-csharp"
      "language-go"
      "language-mustache"
      "language-perl"
      "language-ruby-on-rails"
      "language-sass"
      "language-toml"
      "metrics"
      "open-on-github"
      "package-generator"
      "styleguide"
      "update-package-dependencies"
      "welcome"
    ]
    themes: [
      "native-ui"
      "one-light-syntax"
    ]
  editor:
    invisibles:
      cr: "↩"
      eol: false
      space: false
      tab: "⇥"
    preferredLineLength: 120
    showIndentGuide: true
    showInvisibles: true
    softTabs: true
    tabLength: 4
  minimap:
    plugins:
      "git-diff": true
      selection: true
  "one-dark-ui":
    layoutMode: "Compact"
  "one-light-ui":
    layoutMode: "Compact"
  tabs:
    enableVcsColoring: true
    usePreviewTabs: true
  welcome:
    showOnStartup: false
  whitespace:
    ignoreWhitespaceOnCurrentLine: false
".c.source":
  editor:
    preferredLineLength: 100
    softTabs: false
".cpp.source":
  editor:
    preferredLineLength: 100
    softTabs: false
".lua.source":
  editor:
    tabLength: 2
".python.source":
  editor:
    softTabs: false
".ruby.source":
  editor:
    tabLength: 2
".shell.source":
  editor:
    tabLength: 2
