[core]
	quotepath = false
	autocrlf = input
	safecrlf = warn
	trustctime = false
	excludesfile = ~/.gitexcludes
[grep]
	lineNumber = true
[pack]
	threads = 0
[push]
	default = simple
[branch]
	autosetuprebase = always
[color]
	ui = auto
[svn]
	rmdir = true
[alias]
	wipe = reset --hard HEAD
	cleanup = clean -dxf
	original = !git wipe && git cleanup
	svnc = svn clone
	svnf = svn fetch
	svnr = svn rebase
	svnd = svn dcommit
[credential]
	helper = osxkeychain
[include]
	path = ~/.gitconfig.local